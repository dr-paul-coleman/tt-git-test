    <validationRules>
        <fullName>PreventStatusChangeInQueue</fullName>
        <active>true</active>
        <description>Prevents someone from updating the status of a request if it is owned by the Pro Assistance queue.</description>
        <errorConditionFormula>AND(
Owner:Queue.Id = &apos;00G31000004pyCq&apos;,
ISCHANGED(Status__c)
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot change the status of a Request when it is owned by the Pro Assistance queue. Please take ownership of this request, then update the Status.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>NextRequest</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Next Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}
var reqObj = new sforce.SObject(&quot;Request__c&quot;);
var result = sforce.connection.query(&quot;SELECT id, ownerid,service__r.ProAssistantManagedBy__c FROM Request__c WHERE ownerid = &apos;00G31000004pyCq&apos; AND service__r.ProAssistantManagedBy__c = &apos;Enterprise&apos; ORDER BY createddate ASC LIMIT 1&quot;);
var records = result.getArray(&quot;records&quot;);
if (records.length &gt; 0){
    var id = records[0].Id;
    reqObj.Id = id;
    reqObj.OwnerId = &apos;{!$User.Id}&apos;;
    var result = sforce.connection.update([reqObj]);
    window.open(&quot;/&quot; + id, &quot;_self&quot;);
    
} else {
    var result = sforce.connection.query(&quot;SELECT id, ownerid FROM Request__c WHERE ownerid = &apos;00G31000004pyCq&apos; ORDER BY createddate ASC LIMIT 1&quot;);
    
    var records = result.getArray(&quot;records&quot;);
    if (records.length &gt; 0){
        var id = records[0].Id;
        reqObj.Id = id;
