    <fields>
        <fullName>CatGeo__c</fullName>
        <description>Formula field to display both the category and geo in a single field.

rca_name__c + zip_cbsa_name__c</description>
        <externalId>false</externalId>
        <formula>TRIM(
  IF(
    ISBLANK(rca_name__c),
    &quot;&quot;,
    rca_name__c
  ) + &quot; &quot; +
  IF(
    ISBLANK(zip_cbsa_name__c),
    &quot;&quot;,
    zip_cbsa_name__c
  )
)</formula>
        <label>Cat/Geo</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
