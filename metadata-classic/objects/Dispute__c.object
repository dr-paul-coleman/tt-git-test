    <validationRules>
        <fullName>RequireRequestData</fullName>
        <active>true</active>
        <description>Validation rule that requires a new record to have the Request Category, Request City, Request State, and Request Zip before initial confirmation gets sent to the initiator.</description>
        <errorConditionFormula>ISPICKVAL(Stage__c, &apos;Initiator confirmation needed&apos;) &amp;&amp;
OR(ISBLANK(request_category__c),
     ISBLANK(request_city__c),
     ISBLANK(request_state__c),
     ISBLANK(request_zip__c))</errorConditionFormula>
        <errorMessage>Please add a Request Category, Request City, Request State, and Request Zip before setting the stage to &apos;Initiator confirmation needed&apos;.</errorMessage>
    </validationRules>
