    <fields>
        <fullName>DefaultDisputeSettingsOverridden__c</fullName>
        <description>Checkbox to indicate true if the dispute defaults do not match the related experience defaults indicating they have been overridden by an agent.</description>
        <externalId>false</externalId>
        <formula>IF(MaxofDaysforOffer__c &lt;&gt; DisputeExperience__r.MaxofDaysforOffer__c, True,
IF(MaxOfDaysForInitiatorToConfirm__c &lt;&gt; DisputeExperience__r.MaxOfDaysForInitiatorToConfirm__c, True,
IF(MaxOfInitiatorOffersAllowed__c &lt;&gt; DisputeExperience__r.MaxOfInitiatorOffersAllowed__c, True,
IF(MaxOfResponderOffersAllowed__c &lt;&gt; DisputeExperience__r.MaxOfResponderOffersAllowed__c, True,
IF(MaxOfInitiatorMessagesAllowed__c &lt;&gt; DisputeExperience__r.MaxOfInitiatorMessagesAllowed__c, True,
IF(MaxOfResponderMessagesAllowed__c &lt;&gt; DisputeExperience__r.MaxOfResponderMessagesAllowed__c, True,
IF(MaxOfDaysForInitiatorToReply__c &lt;&gt; DisputeExperience__r.MaxOfDaysForInitiatorToReply__c, True,
IF(MaxOfDaysForResponderToReply__c &lt;&gt; DisputeExperience__r.MaxOfDaysForResponderToReply__c, True,
IF(MaxOfDaysForResponderFirstAction__c &lt;&gt; DisputeExperience__r.MaxOfDaysForResponderFirstAction__c, True,
IF(MaxOfDaysAllowedForDispute__c &lt;&gt; DisputeExperience__r.MaxOfDaysAllowedForDispute__c, True,
IF(MaxDaysForCalendarPicker__c &lt;&gt; DisputeExperience__r.MaxDaysForCalendarPicker__c, True,
False)))))))))))</formula>
        <inlineHelpText>Checkbox to indicate true if the dispute defaults do not match the related experience defaults indicating they have been overridden by an agent.</inlineHelpText>
        <label>Default Dispute Settings Overridden</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
