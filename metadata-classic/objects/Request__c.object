  SUBSTITUTE(EmailSendQuoteLinkPart3__c, &quot;&amp;amp;&quot;,&quot;&amp;&quot;) + 
  SUBSTITUTE(EmailSendQuoteLinkPart4__c, &quot;&amp;amp;&quot;,&quot;&amp;&quot;), 
