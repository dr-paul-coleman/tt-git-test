    <fields>
        <fullName>PDGEligibility__c</fullName>
        <description>Formula field to calculate the eligibility of the property damage guarantee.</description>
        <externalId>false</externalId>
        <formula>IF(OR(
ISPICKVAL(EligibilityFactors__c, &quot;Contract terms met, customer still dissatisfied&quot;),
ISPICKVAL(EligibilityFactors__c, &quot;Damage is to excluded property&quot;), 
