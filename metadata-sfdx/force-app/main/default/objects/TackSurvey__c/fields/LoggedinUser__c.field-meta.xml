<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LoggedinUser__c</fullName>
    <description>Formula field to mark the checkbox true if the logged in user matches the User associated with the Survey Response.</description>
    <externalId>false</externalId>
    <formula>IF(User__r.Id = $User.Id , True, False )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Logged in User</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
