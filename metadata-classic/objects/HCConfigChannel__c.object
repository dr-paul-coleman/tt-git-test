    <fields>
        <fullName>ChannelUnavailableEndTime__c</fullName>
        <description>The end date/time that the channel unavailable message should be removed.</description>
        <externalId>false</externalId>
        <inlineHelpText>Set time to 1 hour after you want the channel to be turned back on.  A time-based workflow will turn the channel on 1 hour before the time set here.  For example, if the phones should be turned back on at 2:00 PM then set the start time to be 3:00 PM.</inlineHelpText>
        <label>Channel Unavailable End Time</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
