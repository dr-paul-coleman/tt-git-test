ISPICKVAL(EligibilityFactors__c, &quot;Other - Eligible (leave notes)&quot;) 
),&quot;Eligible&quot;, 

IF(AND(
MIIncident__r.quotes_hired__c = True, 
MIIncident__r.ElapsedReportedTimeDays__c &lt;= 72, 
DATETIMEVALUE(MIIncident__r.request_create_time__c) &gt;= DATETIMEVALUE( &apos;2016-07-11 12:00:00&apos; ) 
),&quot;May Be Eligible&quot;, 

&quot; Not Eligible &quot; 
