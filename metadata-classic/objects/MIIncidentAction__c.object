    <fields>
        <fullName>Closed__c</fullName>
        <description>Formula to represent if the MI Incident action is in a closed status based on Record Type; used in trigger code to populate &quot;Completed On&quot; and &quot;Completed By&quot; time stamps.</description>
        <externalId>false</externalId>
        <formula>IF(AND(RecordType.Name = &quot;Task&quot;, 
NOT ISPICKVAL(Status__c, &quot;Open&quot;)),True, 
IF(AND(RecordType.Name = &quot;Payout&quot;, 
OR(ISPICKVAL(Status__c,&quot;Payment Sent&quot;), 
ISPICKVAL(Status__c,&quot;Rejected&quot;))),True, 
IF(AND(RecordType.Name = &quot;Guarantee&quot;, 
OR(ISPICKVAL(Status__c, &quot;Completed&quot;), 
ISPICKVAL(Status__c, &quot;Paid (under Property Damage Guarantee)&quot;), 
ISPICKVAL(Status__c, &quot;Paid (under Money Back Guarantee)&quot;), 
ISPICKVAL(Status__c, &quot;Paid (under Satisfaction)&quot;), 
ISPICKVAL(Status__c, &quot;Not paid&quot;))),True, 
IF(AND(RecordType.Name = &quot;Charge Card&quot;, 
NOT ISPICKVAL(Status__c, &quot;Open&quot;)),True, 
False))))</formula>
        <label>Closed</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
