    <fields>
        <fullName>AgreementMadeResolution__c</fullName>
        <description>Formula field used in email templates.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(FinalResolution__c), NULL,
&quot;The agreement between &quot; + InitiatorFirstName__c +&quot; &quot;+ InitiatorLastName__c + &quot; and &quot; + ResponderFirstName__c + &quot; &quot; + ResponderLastName__c + &quot; is: &quot; +
IF(FinalResolution__c = &apos;Complete&apos;, &apos;Complete the work.&apos;,
IF(FinalResolution__c = &apos;Complete or redo the work&apos;, &apos;Complete or redo the work.&apos;,
IF(FinalResolution__c = &apos;Refund&apos;, &apos;Refund in the amount of $&apos; + TEXT(FinalResolutionAmount__c) +&apos;.&apos;,
IF(FinalResolution__c = &apos;CompleteAndRefund&apos;, &apos;Complete the work and provide a refund in the amount of $&apos;+ TEXT(FinalResolutionAmount__c) +&apos;.&apos;,
IF(FinalResolution__c = &apos;Complete And Refund&apos;, &apos;Complete the work and provide a refund in the amount of$ &apos;+ TEXT(FinalResolutionAmount__c) + &apos;.&apos;,
IF(FinalResolution__c = &apos;Complete or redo the work and refund&apos;, &apos;Complete the work and provide a refund in the amount of &apos;+ TEXT(FinalResolutionAmount__c) + &apos;.&apos;,
FinalResolution__c + &apos; $&apos; + Text(FinalResolutionAmount__c)
))))))
)</formula>
        <inlineHelpText>Formula field used in email templates.</inlineHelpText>
        <label>Agreement Made Resolution</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
