        <description>Prevents setting CSAT Eligibility for L1 Categories.</description>
        <errorConditionFormula>RecordType.Name = &quot;Category&quot; &amp;&amp;
ISPICKVAL(CategoryType__c, &quot;Category&quot;) &amp;&amp;
NOT(ISPICKVAL(CSATEligible__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>CSATEligible__c</errorDisplayField>
        <errorMessage>Primary Categories do not require CSAT Eligibility.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventParentCategory</fullName>
        <active>true</active>
        <description>Prevents setting a Parent Category if CategoryType__c = Category</description>
        <errorConditionFormula>RecordType.Name = &quot;Category&quot; &amp;&amp;
ISPICKVAL(CategoryType__c, &quot;Category&quot;) &amp;&amp;
NOT(ISBLANK(ParentCategory__c))</errorConditionFormula>
        <errorDisplayField>ParentCategory__c</errorDisplayField>
        <errorMessage>Primary Categories should not have a Parent Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequireAlias</fullName>
        <active>true</active>
        <description>Requires Alias if category is available in Help Center.</description>
        <errorConditionFormula>RecordType.Name = &quot;Category&quot; &amp;&amp;
AvailableinHelpCenter__c &amp;&amp;
ISBLANK(Alias__c)</errorConditionFormula>
        <errorDisplayField>Alias__c</errorDisplayField>
        <errorMessage>Please provide an alias for use in the Help Center.</errorMessage>
