        <fullName>RequireChurnReason</fullName>
        <active>true</active>
        <description>Enter a Churn Reason and End Date if either field populated</description>
        <errorConditionFormula>AND( $User.ProfileId != &quot;00ei00000011y4A&quot;, 
IF(ISBLANK(EndDate__c), 0, 1) + 
IF(ISBLANK(TEXT(ChurnReason__c)), 0, 1) == 1 )</errorConditionFormula>
        <errorDisplayField>ChurnReason__c</errorDisplayField>
        <errorMessage>Please complete End Date and Churn Reason before saving this record.</errorMessage>
    </validationRules>
